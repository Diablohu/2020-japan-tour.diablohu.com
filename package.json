{
    "name": "2020-japan-tour.diablohu.com",
    "private": true,
    "version": "1.0.0",
    "description": "https://2020-japan-tour.diablohu.com",
    "author": {
        "name": "diablohu",
        "email": "diablohudream@gmail.com"
    },
    "homepage": "https://github.com/cmux/koot#readme",
    "license": "none",
    "repository": {
        "type": "git",
        "url": "https://github.com/cmux/koot.git"
    },
    "engines": {
        "node": ">=10.13.0"
    },
    "scripts": {
        "start": "koot-start",
        "start:server": "koot-start --no-build",
        "build": "koot-build",
        "build:spa": "koot-build --type spa",
        "dev": "koot-dev",
        "dev:no-open": "koot-dev --no-open",
        "dev:spa": "koot-dev --type spa",
        "analyze": "koot-analyze",
        "push": "node ./scripts/push.js",
        "eslint": "eslint \"src/**/*.+(js|jsx)\"",
        "check-types": "tsc --noEmit",
        "up": "yarn upgrade-interactive --latest"
    },
    "dependencies": {
        "axios": "^0.19.1",
        "bind-event": "^1.1.2",
        "classnames": "^2.2.6",
        "koot": "^0.12.0",
        "swiper": "^5.3.0"
    },
    "devDependencies": {
        "@types/classnames": "^2.2.9",
        "@types/gtag.js": "^0.0.3",
        "@types/md5": "^2.1.33",
        "@types/swiper": "^5.2.1",
        "autoprefixer": "^9.7.3",
        "babel-eslint": "^10.0.3",
        "cssnano": "^4.1.10",
        "date-format": "^3.0.0",
        "eslint": "^6.8.0",
        "eslint-config-koot": "^1.6.0",
        "husky": "^4.0.7",
        "lint-staged": "^9.5.0",
        "md5": "^2.2.1",
        "normalize.css": "^8.0.1",
        "prettier": "^1.19.1",
        "prettier-eslint": "^9.0.1",
        "pretty-ms": "^5.1.0",
        "redux-logger": "^3.0.6",
        "sharp": "^0.24.0",
        "svg-url-loader": "^3.0.3",
        "url-loader": "^3.0.0"
    },
    "bugs": {
        "url": "https://github.com/cmux/koot/issues"
    },
    "eslintIgnore": [
        "node_modules/**/*"
    ],
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,cjs,mjs,ts,tsx}": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ],
        "*.{json,md,css,less,sass,scss}": [
            "prettier --write",
            "git add"
        ]
    },
    "koot": {
        "version": "0.12.0",
        "baseVersion": "0.12.0"
    }
}
